{
  "name": "artgris/filemanager-bundle",
  "type": "symfony-bundle",
  "description": "FileManager is a simple Multilingual File Manager Bundle for Symfony",
  "keywords": [
    "symfony",
    "bundle",
    "file manager"
  ],
  "homepage": "https://github.com/artgris/FileManagerBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Gribet",
      "email": "a.gribet@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "sensio/framework-extra-bundle": "~5.1",
    "symfony/translation": "~5.0",
    "symfony/twig-pack": "*",
    "symfony/form": "~5.0",
    "symfony/validator": "~5.0",
    "symfony/asset": "~5.0",
    "oomphinc/composer-installers-extender": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0|~5.0",
    "symfony/security-bundle": "^5.0",
    "symfony/css-selector": "^5.0",
    "symfony/browser-kit": "^5.0",
    "symfony/var-dumper": "^5.0",
    "symfony/yaml": "^5.0",
    "symfony/templating":  "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Artgris\\Bundle\\FileManagerBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Artgris\\Bundle\\FileManagerBundle\\Tests\\": "/Tests/"
    }
  }
}
